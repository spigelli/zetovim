{
  "git.repositoryScanIgnoredFolders": [
    "node_modules",
  ],
  "Lua.runtime.version": "Lua 5.2",
  "Lua.runtime.builtin": {
    "basic": "default",
    "bit": "enable",
    "bit32": "default",
    "builtin": "default",
    "coroutine": "default",
    "debug": "default",
    "ffi": "default",
    "io": "default",
    "jit": "enable",
    "math": "default",
    "os": "default",
    "package": "default",
    "string": "default",
    "string.buffer": "default",
    "table": "default",
    "table.clear": "default",
    "table.new": "default",
    "utf8": "default"
  },
  "Lua.codeLens.enable": true,
  "Lua.completion.displayContext": 10,
  "Lua.completion.keywordSnippet": "Disable",
  "Lua.runtime.pathStrict": true,
  "Lua.runtime.path": [
    ".lazy/?.nvim/lua/?/init.lua",
    "?.lua",
    "?/init.lua",
    
  ],
  "Lua.workspace.ignoreDir": [
    ".lazy", 
    "types"
  ],
  
  "Lua.diagnostics.libraryFiles": "Disable",
  "Lua.workspace.library": [
    "types",
    ".lazy",
  ],
  "Lua.workspace.checkThirdParty": true,
  "Lua.diagnostics.disable": [
    "duplicate-doc-field",
    "duplicate-doc-alias",
    "duplicate-set-field"
  ],
  "Lua.hover.previewFields": 10,
  "Lua.workspace.maxPreload": 1000,
}