{
  "git.repositoryScanIgnoredFolders": [
    "node_modules",
  ],
  "Lua.runtime.version": "Lua 5.2",
  "Lua.runtime.builtin": {
    "basic": "default",
    "bit": "enable",
    "bit32": "default",
    "builtin": "default",
    "coroutine": "default",
    "debug": "default",
    "ffi": "default",
    "io": "default",
    "jit": "enable",
    "math": "default",
    "os": "default",
    "package": "default",
    "string": "default",
    "string.buffer": "default",
    "table": "default",
    "table.clear": "default",
    "table.new": "default",
    "utf8": "default"
  },
  "Lua.codeLens.enable": true,
  "Lua.completion.displayContext": 10,
  "Lua.completion.keywordSnippet": "Disable",
  "Lua.runtime.pathStrict": true,
  "Lua.runtime.path": [
    ".lazy/?.nvim/lua/?/init.lua",
    "?.lua",
    "?/init.lua",
  ],
  "Lua.workspace.ignoreDir": [
    ".lazy", 
    "types"
  ],
  
  "Lua.diagnostics.libraryFiles": "Disable",
  "Lua.workspace.library": [
    "types",
    ".lazy",
    "/home/spigelli/nvim/share/nvim/runtime/lua",
  ],
  "Lua.workspace.checkThirdParty": true,
  "Lua.diagnostics.disable": [
    "duplicate-doc-field",
    "duplicate-doc-alias",
    "duplicate-set-field"
  ],
  "Lua.hover.previewFields": 10,
  "editor.wordWrapColumn": 60,
  "luahelper.format.column_limit": 60,
  "luahelper.format.align_args": true,
  "luahelper.format.align_parameter": true,
  "luahelper.format.break_after_functioncall_lp": true,
  "luahelper.format.break_after_functiondef_lp": true,
  "luahelper.format.break_after_table_lb": true,
  "luahelper.format.break_before_functioncall_rp": true,
  "luahelper.format.break_before_table_rb": true,
  "luahelper.format.chop_down_parameter": true,
  "luahelper.format.chop_down_table": true,
  "luahelper.format.continuation_indent_width": 2,
  "luahelper.format.double_quote_to_single_quote": true,
  "luahelper.format.extra_sep_at_table_end": true,
  "luahelper.format.indent_width": 2,
  "luahelper.format.keep_simple_function_one_line": true,
  "luahelper.format.line_breaks_after_function_body": 1,
  "luahelper.format.spaces_inside_functioncall_parens": true,
  "luahelper.format.spaces_inside_table_braces": true,
  "luahelper.format.tab_width": 2,
  "luahelper.base.PreviewFieldsNum": 0,
  "luahelper.base.formatErrShow": true,
  "editor.wordWrap": "wordWrapColumn",
}